    <attribut isComposite="true">
      <jmxAttribut>CollectionUsage</jmxAttribut>
      <description>CollectionUsage</description>
      <name>CollectionUsage</name>
      <keys>
    	  <key>used</key>
       	<description> Eden Space:used 	</description>
       </keys>    
             <keys>
    	  <key>max</key>
       	<description> Eden Space:max 	</description>
       </keys> 
    </attribut>
    <attribut >
          <jmxAttribut>CollectionUsageThreshold</jmxAttribut>
      <description>CollectionUsageThreshold</description>
      <name>CollectionUsageThreshold</name>
    </attribut >
  </counter>
</tns:monitorConfig>
